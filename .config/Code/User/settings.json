{
  // Настройки редактора
  "editor.quickSuggestions": {
    "comments": true,
    "strings": true,
    "other": true
  },
  "editor.accessibilitySupport": "off",
  "editor.autoIndent": "full",
  "editor.bracketPairColorization.enabled": true,
  "editor.colorDecorators": true,
  "editor.copyWithSyntaxHighlighting": false,
  "editor.cursorSmoothCaretAnimation": "explicit",
  "editor.folding": false,
  "editor.foldingStrategy": "auto",
  "editor.fontFamily": "Iosevka NF",
  "editor.fontLigatures": true,
  "editor.lineHeight": 1.9,
  "editor.lineNumbers": "on",
  "editor.matchBrackets": "never",
  "editor.minimap.enabled": false,
  "editor.mouseWheelScrollSensitivity": 4,
  "editor.parameterHints.enabled": false,
  "editor.renderLineHighlight": "none",
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.horizontalScrollbarSize": 5,
  "editor.selectionHighlight": false,
  "editor.smoothScrolling": true,
  "editor.snippetSuggestions": "inline",
  "editor.tabCompletion": "on",
  "editor.wordWrap": "off",
  "editor.inlineSuggest.enabled": true,
  "editor.hover.enabled": true,
  "editor.lightbulb.enabled": "off",
  "editor.stickyScroll.enabled": false,
  "editor.suggestFontSize": 13,
  "editor.suggestLineHeight": 27,
  "editor.glyphMargin": false,
  "editor.hideCursorInOverviewRuler": true,
  "editor.scrollbar.verticalScrollbarSize": 5,
  "editor.gotoLocation.multipleReferences": "goto",
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.gotoLocation.multipleDeclarations": "goto",
  "editor.gotoLocation.multipleImplementations": "goto",
  "editor.gotoLocation.multipleTypeDefinitions": "goto",

  // Настройки интерфейса
  "workbench.colorTheme": "Everforest Dark",
  "workbench.editor.showIcons": true,
  "workbench.editor.showTabs": "multiple",
  "workbench.editor.tabSizing": "shrink",
  "workbench.panel.defaultLocation": "bottom",
  "workbench.statusBar.visible": true,
  "workbench.tips.enabled": false,
  "workbench.tree.indent": 10,
  "workbench.view.alwaysShowHeaderActions": true,
  "workbench.tree.renderIndentGuides": "none",
  "workbench.editor.pinnedTabSizing": "compact",
  "workbench.colorCustomizations": {
    "inlineparameters.annotationBackground": "#282A3600",
    "inlineparameters.annotationForeground": "#7b82af",
    "scrollbar.shadow": "#24292e00"
  },

  "workbench.list.smoothScrolling": true,
  "workbench.sideBar.location": "right",
  "workbench.editor.labelFormat": "short",
  "workbench.localHistory.maxFileSize": 1000,
  "workbench.activityBar.orientation": "vertical",
  "workbench.editor.enablePreview": false,
  "workbench.iconTheme": "ayu",
  "workbench.settings.applyToAllProfiles": [],

  "problems.decorations.enabled": false,
  "editor.overviewRulerBorder": false,

  // Настройки терминала
  "terminal.integrated.fontFamily": "Iosevka",
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.gpuAcceleration": "on",
  "terminal.integrated.lineHeight": 1.5,
  "terminal.integrated.tabs.enabled": true,
  "terminal.integrated.tabs.location": "right",

  // Files
  "explorer.decorations.badges": false,
  "explorer.compactFolders": false,
  "explorer.fileNesting.expand": false,
  "explorer.decorations.colors": true,

  // Настройки Git
  "git.enableCommitSigning": true,
  "git.autofetch": true,
  "git.decorations.enabled": true,

  // Настройки поиска
  "search.useIgnoreFiles": false,
  "search.exclude": {
    "**/node_modules": true,
    "**/dist": true,
    "**debug": true,
    "**/package-lock.json": true,
    "**/.next": true,
    "**/data": true
  },

  // Настройки файлов
  "files.restoreUndoStack": false,
  "files.insertFinalNewline": true,
  "files.exclude": {
    "**/**.moc": true,
    "**/**.o": true,
    "**/moc_**.cpp": true,
    "**/moc_**.h": true,
    "**/qrc_**.cpp": true,
    "**/**.pyc": true
  },

  // Настройки отладки
  "debug.toolBarLocation": "floating",
  "debug.enableStatusBarColor": true,
  "debug.console.fontFamily": "Iosevka NF",
  "debug.disassemblyView.showSourceCode": false,
  "debug.console.fontSize": 12,
  "debug.console.lineHeight": 20,

  // Настройки SCM
  "scm.diffDecorations": "gutter",
  "scm.showIncomingChanges": "always",
  "scm.showOutgoingChanges": "always",


  // Настройки окна
  "window.menuBarVisibility": "compact",
  "window.title": " ",
  "window.dialogStyle": "custom",
  "window.titleBarStyle": "custom",
  "window.density.editorTabHeight": "default",
  "window.zoomLevel": 3,
  "window.customTitleBarVisibility": "auto",
  "window.commandCenter": false,
  "window.nativeFullScreen": true,
  "window.nativeTabs": false,

  "editor.guides.indentation": false,
  "workbench.editor.empty.hint": "hidden",
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "name": "One Dark italic",
        "scope": [
          "comment",
          "entity.other.attribute-name",
          "keyword",
          "markup.underline.link",
          "storage.modifier",
          "storage.type",
          "string.url",
          "variable.language.super",
          "variable.language.this"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "name": "One Dark italic reset",
        "scope": [
          "keyword.operator",
          "keyword.other.type",
          "storage.modifier.import",
          "storage.modifier.package",
          "storage.type.built-in",
          "storage.type.function.arrow",
          "storage.type.generic",
          "storage.type.java",
          "storage.type.primitive"
        ],
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  },
  "chat.commandCenter.enabled": false,
  "errorLens.borderRadius": "12px",
  "errorLens.fontWeight": "900",
  "errorLens.messageBackgroundMode": "message",
  "errorLens.replaceLinebreaksSymbol": "- ",
  "errorLens.searchForProblemQuery": "https://google.com/?q=$message",
  "editor.fontSize": 13,
  "workbench.productIconTheme": "bongocat",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[astro]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "codeium.enableCodeLens": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.layoutControl.enabled": false,
  "errorLens.editorHoverPartsEnabled": {
    "messageEnabled": true,
    "buttonsEnabled": true,
    "sourceCodeEnabled": true
  },
  "errorLens.fontFamily": "Iosevka NF",
  "errorLens.gutterIconSet": "squareRounded",
  "errorLens.statusBarMessageTemplate": "$count",
  "errorLens.gutterIconsEnabled": true,
  "errorLens.margin": "0px",
  "errorLens.severityText": [],
  "errorLens.statusBarIconsAtZero": "hide",
  "errorLens.respectUpstreamEnabled": {
    "statusBar": true
  },
  "eslint.alwaysShowStatus": true,
  "eslint.lintTask.enable": true,
  "eslint.lintTask.options": ". --ext .ts --ext .tsx",
  "errorLens.excludeBySource": ["ts(1239)"],

  "workbench.startupEditor": "none",
  "editor.cursorBlinking": "phase",

  "breadcrumbs.enabled": false,
  "breadcrumbs.filePath": "on",
  "breadcrumbs.showArrays": false,
  "breadcrumbs.showBooleans": false,
  "breadcrumbs.showClasses": false,
  "breadcrumbs.showConstants": false,
  "breadcrumbs.showConstructors": false,
  "breadcrumbs.showEnumMembers": false,
  "breadcrumbs.showEnums": false,
  "breadcrumbs.showEvents": false,
  "breadcrumbs.showFields": false,
  "breadcrumbs.showFiles": false,
  "breadcrumbs.showFunctions": false,
  "breadcrumbs.showInterfaces": false,
  "breadcrumbs.showKeys": false,
  "breadcrumbs.showMethods": false,
  "breadcrumbs.showModules": false,
  "breadcrumbs.showNamespaces": false,
  "breadcrumbs.showNull": false,
  "breadcrumbs.showNumbers": false,
  "breadcrumbs.showObjects": false,
  "breadcrumbs.showOperators": false,
  "breadcrumbs.icons": false,
  "breadcrumbs.showPackages": false,
  "breadcrumbs.showProperties": false,
  "breadcrumbs.showStrings": false,
  "breadcrumbs.showStructs": false,
  "breadcrumbs.showTypeParameters": false,
  "breadcrumbs.showVariables": false,
  "breadcrumbs.symbolPath": "off",

  "apc.listRow": {
    "height": 28,
    "fontSize": 14
  },
  "apc.sidebar.titlebar": {
    "fontSize": 0
  },
  "apc.statusBar": {
    "fontSize": 13,
    "height": 25
  },

  "vscode_custom_css.imports": [
    "file:///home/nighty/.config/Code/User/custom.css"
  ],
  "workbench.activityBar.location": "top",
}
