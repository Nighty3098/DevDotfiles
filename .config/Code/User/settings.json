{
  "vscode_custom_css.imports": [
    "file:///home/nighty/.config/Code/User/style.css",
    "file:///home/nighty/.config/Code/User/settings.css"
  ],
  "animations.Install-Method": "Apc Customize UI++",
  "window.title": "~\\${activeFolderMedium}",
